{
  "name": "travelfeed-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "watch": "nodemon --exec \"npm run build && npm run start\" --watch src --ext ts",
    "lint": "tslint -p tsconfig.json",
    "test": "ava tests/**/*.js",
    "format": "prettier --write \"src/**/*.{ts,scss,md}\"",
    "precommit": "lint-staged"
  },
  "private": true,
  "lint-staged": {
    "{e2e,src}/**/*.{ts,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "acl": "^0.4.11",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "decko": "^1.2.0",
    "express": "^4.16.2",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.0",
    "knex": "^0.14.4",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "typeorm": "^0.1.16",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.3",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.37",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/knex": "^0.14.8",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.2",
    "@types/node": "^9.4.6",
    "@types/passport": "^0.4.3",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.4",
    "ava": "^0.25.0",
    "prettier": "^1.11.1",
    "reflect-metadata": "^0.1.12",
    "supertest": "^3.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "typescript": "^2.7.2"
  }
}
