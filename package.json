{
  "name": "travelfeed-api",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "watch": "nodemon --exec \"npm run build && npm run start\" --watch src --ext ts",
    "lint": "tslint -p tsconfig.json --fix",
    "test": "ava tests/**/*.js",
    "format": "prettier --write \"src/**/*.{ts,scss,md}\"",
    "docs": "typedoc --mode file --theme minimal --gitRevision master --out docs --name 'Travelfeed API' --readme README.md --module commonjs src",
    "precommit": "lint-staged"
  },
  "private": true,
  "lint-staged": {
    "{e2e,src}/**/*.{ts,scss,md}": [
      "prettier --write",
      "tslint -p tsconfig.json --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/ejs": "^2.5.1",
    "@types/nodemailer": "^4.6.0",
    "acl": "^0.4.11",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "decko": "^1.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.2.1",
    "knex": "^0.14.6",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.6.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "routing-controllers": "^0.7.7",
    "typedi": "^0.7.3",
    "typeorm": "^0.2.6",
    "typeorm-routing-controllers-extensions": "^0.2.0",
    "typeorm-typedi-extensions": "^0.2.1",
    "uuid": "^3.2.1",
    "validator": "^10.2.0",
    "winston": "winstonjs/winston#master"
  },
  "devDependencies": {
    "@types/acl": "^0.4.34",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.37",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/knex": "^0.14.14",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.4",
    "@types/node": "^10.1.3",
    "@types/passport": "^0.4.5",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "ava": "^0.25.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.0",
    "reflect-metadata": "^0.1.12",
    "supertest": "^3.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  }
}
